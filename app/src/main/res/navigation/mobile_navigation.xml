<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.bottom_nav_shell.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_welcome"
        android:name="com.example.bottom_nav_shell.WelcomeFragment"
        android:label="Welcome"
        tools:layout="@layout/welcome_fragment" />

    <fragment
        android:id="@+id/navigation_result"
        android:name="com.example.bottom_nav_shell.ResultFragment"
        android:label="Result"
        tools:layout="@layout/result_fragment" />
</navigation>